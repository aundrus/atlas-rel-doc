#!/bin/sh
function show_help() {
    echo "ardoc_post_art_results: Error: incorrect option(s)"
}
###########
comname=`basename $0`
if [ "$comname" = "ardoc_post_art_results" ]; then
exitcomtool="exit"
else
exitcomtool="return"
fi

this_dir=`dirname $0`
export ARDOC_HOME="$this_dir"
#
#dateString=`date +%Y-%m-%d-%H-%M`
#epoch=`date +%s.%N`
relDateStamp=""
nname=""

while [ $# -ne 0 ]; do
    case $1 in
         --relstamp) relDateStamp=$2; shift;;
         -n | --nightly)   nname=$2; shift;;
         -* | --*)         show_help; ${exitcomtool} 1;;
    esac
    shift
done

if [ "${ARDOC_HOME}" = "" ]; then
echo "ardoc_job: Error: ARDOC_HOME variable is not defined, "
echo "           please set before running ardoc jobs"
eval ${exitcomjob} 1
fi

echo "ardoc_post_art_results: INFO: start" `date`

$ARDOC_HOME/ardoc_oracle_wrapper.sh 3 python $ARDOC_HOME/ardoc_post_art_results.py
st=0
${exitcomtool} ${st}

